    {
      "cell_type": "code",
      "source": [
        "import folium\n",
        "from folium.plugins import HeatMap\n",
        "import pandas as pd\n",
        "\n",
        "# 1. Préparer les données\n",
        "incident_response = df[df['FirstPumpArriving_AttendanceTime'].notna()].copy()\n",
        "incident_response['TempsReponse_min'] = incident_response['FirstPumpArriving_AttendanceTime'] / 60\n",
        "incident_response = incident_response.drop_duplicates(subset='IncidentNumber')\n",
        "\n",
        "# 2. Nettoyer Latitude et Longitude : remplacer ',' par '.'\n",
        "for col in ['Latitude', 'Longitude']:\n",
        "    if col in incident_response.columns:\n",
        "        incident_response[col] = incident_response[col].astype(str).str.replace(',', '.')\n",
        "        incident_response[col] = pd.to_numeric(incident_response[col], errors='coerce')\n",
        "\n",
        "# 3. Supprimer les lignes sans coordonnées valides\n",
        "incident_response = incident_response.dropna(subset=['Latitude', 'Longitude'])\n",
        "\n",
        "# 4. Vérifier que c'est bon\n",
        "print(f\"Nombre d'incidents avec coordonnées valides : {len(incident_response)}\")\n",
        "print(\"Exemple de coordonnées :\")\n",
        "print(incident_response[['Latitude', 'Longitude']].head(3))\n",
        "\n",
        "# 5. Créer la carte\n",
        "m = folium.Map(\n",
        "    location=[51.5074, -0.1278],\n",
        "    zoom_start=11,\n",
        "    tiles='CartoDB positron'\n",
        ")\n",
        "\n",
        "# 6. HeatMap avec poids = temps de réponse\n",
        "heat_data = []\n",
        "for _, row in incident_response.iterrows():\n",
        "    weight = min(row['TempsReponse_min'], 15)\n",
        "    heat_data.append([row['Latitude'], row['Longitude'], weight])\n",
        "\n",
        "HeatMap(\n",
        "    heat_data,\n",
        "    min_opacity=0.3,\n",
        "    radius=12,\n",
        "    blur=15,\n",
        "    gradient={0.2: 'blue', 0.4: 'cyan', 0.6: 'yellow', 0.8: 'orange', 1.0: 'red'}\n",
        ").add_to(m)\n",
        "\n",
        "# 7. Légende\n",
        "legend_html = '''\n",
        "<div style=\"position: fixed; bottom: 50px; left: 50px; width: 180px; height: 120px;\n",
        "            background-color: white; border:2px solid grey; z-index:9999; font-size:14px;\n",
        "            padding: 10px; border-radius: 5px;\">\n",
        "  <b>Temps de réponse</b><br>\n",
        "  <i style=\"background: blue; width: 15px; height: 15px; display: inline-block;\"></i> &lt; 3 min<br>\n",
        "  <i style=\"background: cyan; width: 15px; height: 15px; display: inline-block;\"></i> 3–6 min<br>\n",
        "  <i style=\"background: yellow; width: 15px; height: 15px; display: inline-block;\"></i> 6–9 min<br>\n",
        "  <i style=\"background: orange; width: 15px; height: 15px; display: inline-block;\"></i> 9–12 min<br>\n",
        "  <i style=\"background: red; width: 15px; height: 15px; display: inline-block;\"></i> &gt; 12 min\n",
        "</div>\n",
        "'''\n",
        "m.get_root().html.add_child(folium.Element(legend_html))\n",
        "\n",
        "# 8. Sauvegarder\n",
        "m.save('london_fire_response_heatmap.html')\n",
        "print(\"CARTE SAUVEGARDÉE : london_fire_response_heatmap.html\")"
