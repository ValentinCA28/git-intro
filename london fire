        "import geopandas as gpd\n",
        "import os\n",
        "\n",
        "# Trouver TOUS les .shp dans les sous-dossiers\n",
        "shp_files = []\n",
        "for root, dirs, files in os.walk('heat_map_data'):\n",
        "    for file in files:\n",
        "        if file.endswith('.shp'):\n",
        "            shp_files.append(os.path.join(root, file))\n",
        "\n",
        "print(f\"{len(shp_files)} fichiers .shp trouvés\")\n",
        "for f in shp_files[:5]:\n",
        "    print(f\"  - {f}\")\n",
        "\n",
        "# Charger le PRINCIPAL (celui avec \"Heat_Supply_20120411\")\n",
        "main_shp = [f for f in shp_files if 'Heat_Supply_20120411' in f][0]\n",
        "print(f\"\\nFichier principal : {main_shp}\")\n",
        "\n",
        "gdf_heat = gpd.read_file(main_shp)\n",
        "gdf_heat = gdf_heat.to_crs('EPSG:4326')\n",
        "\n",
        "print(f\"Sources de chaleur chargées : {len(gdf_heat)}\")\n",
        "print(\"Colonnes :\", gdf_heat.columns.tolist())"
