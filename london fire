    {
      "cell_type": "code",
      "source": [
        "# === FUSION + ANALYSE + CARTE ===\n",
        "import pandas as pd\n",
        "from shapely.geometry import Point\n",
        "import folium\n",
        "from folium.plugins import HeatMap\n",
        "\n",
        "# Tes incidents\n",
        "incident_response = df[df['FirstPumpArriving_AttendanceTime'].notna()].copy()\n",
        "incident_response['TempsReponse_min'] = incident_response['FirstPumpArriving_AttendanceTime'] / 60\n",
        "incident_response = incident_response.drop_duplicates('IncidentNumber')\n",
        "for col in ['Latitude', 'Longitude']:\n",
        "    incident_response[col] = incident_response[col].astype(str).str.replace(',', '.')\n",
        "    incident_response[col] = pd.to_numeric(incident_response[col], errors='coerce')\n",
        "incident_response = incident_response.dropna(subset=['Latitude', 'Longitude'])\n",
        "\n",
        "gdf_inc = gpd.GeoDataFrame(\n",
        "    incident_response,\n",
        "    geometry=gpd.points_from_xy(incident_response.Longitude, incident_response.Latitude),\n",
        "    crs='EPSG:4326'\n",
        ")\n",
        "\n",
        "# Buffer + join\n",
        "gdf_heat_buffer = gdf_heat.copy()\n",
        "gdf_heat_buffer.geometry = gdf_heat_buffer.buffer(0.0045)\n",
        "joined = gpd.sjoin(gdf_inc, gdf_heat_buffer, how='left', predicate='intersects')\n",
        "joined['ProcheChaleur'] = ~joined.index_right.isna()\n",
        "\n",
        "# Stats\n",
        "print(f\"{joined['ProcheChaleur'].sum()} / {len(gdf_inc)} incidents près (<500m) → {joined['ProcheChaleur'].mean()*100:.1f}%\")\n",
        "print(f\"Temps moyen près : {joined[joined['ProcheChaleur']]['TempsReponse_min'].mean():.2f} min\")\n",
        "print(f\"Temps moyen loin : {joined[~joined['ProcheChaleur']]['TempsReponse_min'].mean():.2f} min\")\n",
        "\n",
        "# Carte\n",
        "m = folium.Map([51.5074, -0.1278], zoom_start=11)\n",
        "for _, r in gdf_heat.iterrows():\n",
        "    folium.CircleMarker([r.geometry.y, r.geometry.x], radius=5, color='orange', fill=True).add_to(m)\n",
        "proche = joined[joined['ProcheChaleur']]\n",
        "if len(proche)>0:\n",
        "    HeatMap([[r.geometry.y, r.geometry.x] for _, r in proche.iterrows()], radius=12, gradient={1:'red'}).add_to(m)\n",
        "m.save('resultat_final.html')\n",
        "from google.colab import files\n",
        "files.download('resultat_final.html')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 141
        },
