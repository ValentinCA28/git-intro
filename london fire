        "# === VERSION SIMPLE ET FONCTIONNELLE ===\n",
        "\n",
        "# 1. PROJETER EN MÈTRES\n",
        "gdf_heat_m = gdf_heat.to_crs('EPSG:27700')\n",
        "gdf_inc_m = gdf_inc.to_crs('EPSG:27700')\n",
        "\n",
        "# 2. BUFFER 500m\n",
        "gdf_heat_buffer = gdf_heat_m.copy()\n",
        "gdf_heat_buffer.geometry = gdf_heat_buffer.buffer(500)\n",
        "\n",
        "# 3. JOINTURE\n",
        "joined_m = gpd.sjoin(gdf_inc_m, gdf_heat_buffer, how='left', predicate='intersects')\n",
        "joined_m['ProcheChaleur'] = ~joined_m['index_right'].isna()\n",
        "\n",
        "# 4. REVENIR EN LAT/LON\n",
        "joined = joined_m.to_crs('EPSG:4326')\n",
        "\n",
        "# 5. ANALYSE\n",
        "print(f\"{joined['ProcheChaleur'].sum()} / {len(gdf_inc)} incidents près (<500m)\")\n",
        "print(f\"Pourcentage : {joined['ProcheChaleur'].mean()*100:.1f}%\")\n",
        "print(f\"Temps moyen près : {joined[joined['ProcheChaleur']]['TempsReponse_min'].mean():.1f} min\")\n",
        "print(f\"Temps moyen loin : {joined[~joined['ProcheChaleur']]['TempsReponse_min'].mean():.1f} min\")"
